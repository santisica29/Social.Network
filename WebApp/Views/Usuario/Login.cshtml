<h1>Login</h1>
<div class="row">
	<div class="col-md-4">
		<form method="POST" id="formularioLogin">
			<div class="form-group">
				<label for="email" class="control-label">Email</label>
				<input type="email" name="email" id="email" class="form-control" value="santisica29@gmail.com" />
			</div>
			<div class="form-group mt-2">
				<label for="password" class="control-label">Contraseña</label>
				<input type="password" name="password" id="password" class="form-control" value="santisica" /><br />
			</div>
			<div class="form-group">
				<input type="submit" value="Login" class="btn btn-primary" />
			</div>
		</form>
	</div>
</div>

<p id="error-msg">@ViewBag.msg</p>

<script src="~/js/Usuario/login.js"></script>